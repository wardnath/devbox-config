{
  "packages": [
    "atuin@latest",
    "bat@latest",
    "btop@latest",
    "cheat@latest",
    "cloc@latest",
    "cmake@latest",
    "direnv@latest",
    "duf@latest",
    "eza@latest",
    "fzf@latest",
    "gcc@latest",
    "gh@latest",
    "git@latest",
    "glab@latest",
    "go@latest",
    "htop@latest",
    "jq@latest",
    "neofetch@latest",
    "neovim@latest",
    "nmap@latest",
    "nnn@latest",
    "nodejs@latest",
    "pstree@latest",
    "pv@latest",
    "ripgrep@latest",
    "sherlock@latest",
    "starship@latest",
    "thefuck@latest",
    "tig@latest",
    "tldr@latest",
    "tmux@latest",
    "tree-sitter@latest",
    "tree@latest",
    "wget@latest",
    "yq@latest",
    "kubernetes-helm@latest",
    "kubectl@latest",
    "hurl@latest",
    "httpie@latest",
    "navi@latest",
    "chezmoi@latest",
    "projectable@latest",
    "ncdu@latest",
    "p7zip@latest",
    "unzip@latest",
    "trippy@latest",
    "glow@latest",
    "zoxide@latest",
    "asdf@latest",
    "rustup@latest",
    "libiconv@latest",
    "croc@latest",
    "pdm@latest",
    "ffmpeg@latest"
  ],
  "env": {
    "DEVBOX_GLOBAL_PREFIX": "$HOME/.local/share/devbox/global/default/.devbox/nix/profile/default",
    "DEVBOX_GLOBAL_ROOT": "$HOME/.local/share/devbox/global/current"
  },
  "shell": {
    "init_hook": [
      ". ${DEVBOX_GLOBAL_ROOT}/init.sh"
    ],
    "scripts": {
      "install-nnn": [
        "sh -c \"$(curl -Ls https://raw.githubusercontent.com/jarun/nnn/master/plugins/getplugs)\""
      ],
      "install-tmux": [
        "[ -f ~/.tmux/plugins/tpm ] && git clone https://github.com/tmux-plugins/tpm ~/.tmux/plugins/tpm",
        "cp $DEVBOX_GLOBAL_ROOT/dotfiles/.tmux.conf $HOME/.tmux.conf"
      ],
      "install-chezmoi": [
        "chezmoi init --apply --verbose --progress true --source ${DEVBOX_GLOBAL_ROOT}/.local/share/chezmoi/"
      ],
      "latest-hash": [
        "curl -fsSL https://nixos.org/channels/nixos-unstable/git-revision"
      ]
  },
  "nixpkgs": {}
  }
}
